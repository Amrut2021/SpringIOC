<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean class="com.Entity.Students" id="std">
		<property name="id" value="1"></property>
		<property name="name" value="Amruta"></property>
		<property name="city" value="Pune"></property>
		<property name="age" value="24"></property>
		<property name="con" ref="c"></property>

	</bean>

	<bean class="com.Entity.Country" id="c">

		<property name="c_id" value="91"></property>
		<property name="c_name" value="India"></property>

	</bean>
</beans>